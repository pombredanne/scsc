check process <%= @process %> with pidfile <%= @pidfile %>
  start program = "<%= @start %>"
  stop program = "<%= @stop %>"
  if failed host localhost port <%= @port %> type <%= @type %> <% unless @protocol.nil? %>protocol <%= @protocol %><% end %>
    with timeout 10 seconds
    then restart
  if 5 restarts within 5 cycles then timeout
